{
    "name": "preemstudio/payload",
    "description": "A package to easily (de)serialise various types of data.",
    "keywords": [
        "PreemStudio",
        "laravel",
        "payload"
    ],
    "homepage": "https://github.com/preemstudio/payload",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@preem.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "league/csv": "^9.8",
        "netresearch/jsonmapper": "^4.1",
        "spatie/laravel-package-tools": "^1.1",
        "symfony/property-access": "^6.2",
        "symfony/serializer": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.2",
        "graham-campbell/analyzer": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.2",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PreemStudio\\Payload\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PreemStudio\\Payload\\PayloadServiceProvider"
            ],
            "aliases": {
                "Payload": "PreemStudio\\Payload\\Facades\\Payload"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
